{
  "name": "photonews",
  "version": "0.0.1",
  "description": "PhotoNews web app showcasing modown components",
  "main": "app.js",
  "dependencies": {
    "debug": "*",
    "es6-module-transpiler": "~0.3.4",
    "express": "3.x",
    "express-annotations": "*",
    "express-map": "*",
    "express-state": "*",
    "express-view": "*",
    "express-yui": "~1.0.0",
    "grunt-es6-module-transpiler": "git://github.com/caridy/grunt-es6-module-transpiler.git",
    "locator": "*",
    "locator-handlebars": "*",
    "locator-yui": "0.1.x",
    "yui": "3.x"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-watch": "~0.5.3",
    "jshint": "~1.1.0",
    "yui-lint": "~0.1.3"
  },
  "directories": {
    "lib": "./lib"
  },
  "keywords": [
    "express",
    "javascript",
    "modown",
    "photonews"
  ],
  "scripts": {
    "lint": "node ./node_modules/.bin/jshint --config ./node_modules/yui-lint/jshint.json ./lib/*.js *.js",
    "pretest": "npm run lint",
    "xtest": "node ./node_modules/.bin/jshint --config ./node_modules/yui-lint/jshint.json ./lib/*.js ./models/*.js ./public/**/*.js ./routes/*.js ./views/*.js"
  },
  "locator": {
    "-rulesets": "app-rulesets.js"
  },
  "repository": "https://github.com/imalberto/photonews",
  "author": "alberto chan <imalberto@gmail.com>",
  "license": "BSD"
}
